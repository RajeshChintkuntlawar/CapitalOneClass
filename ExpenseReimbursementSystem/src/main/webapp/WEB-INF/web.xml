<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name>ExpenseReimbursementSystem</display-name>
	<servlet>
		<servlet-name>ExpenseReimbursementSystem</servlet-name>
		<servlet-class>com.capitalone.ers.controllers.FrontController</servlet-class>
		<!-- <load-on-startup>1</load-on-startup> -->
	</servlet>
	<servlet-mapping>
		<servlet-name>ExpenseReimbursementSystem</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>

	<error-page>
		<exception-type>com.capitalone.exceptions.UrlNotMappedException</exception-type>
		<location>/static/ERLogin.html</location>
	</error-page>
</web-app>